//Print a pyramid where each row contains increasing numbers starting from 1 up to the row number.

import java.util.*;
public class Main{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    long n=sc.nextLong();
    if((n < 0) || (n >= 50))
    {
      System.out.println("Invalid input");}
     else{
     for(long i=1;i<=n;i++){
       long sum=1;
      for(long j=1;j<=i;j++){
       
        System.out.print(sum);
        sum++;
    }
    
    System.out.println();
  }}
  
}}
