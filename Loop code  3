// output
//     A
//    B B
//   C C C
//  D D D D

import java.util.*;
public class Main{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
   
    if(n<0 || n>=26){
      System.out.println("Invalid input");
      return;
      
    }else{
      for(int i=1;i<=n;i++){
    char ch='A';
    for(int j=1;j<=i;j++){
      System.out.print(ch);
      ch++;
   } System.out.println();
  }}
}}




